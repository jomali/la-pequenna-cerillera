		2:	"Tu soplido no produce ningún efecto.";
		3:	"Su soplido no produce ningún efecto.";
		4:	"Mi soplido no produjo ningún efecto.";
		5:	"Tu soplido no produjo ningún efecto.";
		6:	"Su soplido no produjo ningún efecto.";
		}

